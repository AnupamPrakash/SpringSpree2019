<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.springspree.nitw.springspree2019">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/launcher_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/launcher_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.springspree.nitw.springspree2019.CoreTeam"></activity>
        <activity android:name="com.springspree.nitw.springspree2019.ProshowOpen" />
        <activity android:name="com.springspree.nitw.springspree2019.Proshow" />
        <activity android:name="com.springspree.nitw.springspree2019.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
                <action android:name="android.intent.action.VIEW"/>
            </intent-filter>
        </activity>
        <activity android:name="com.springspree.nitw.springspree2019.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.springspree.nitw.springspree2019.SignupActivity" />
        <activity android:name="com.springspree.nitw.springspree2019.EventSubCategory" />
        <activity android:name="com.springspree.nitw.springspree2019.DetailsActivity" />
        <activity android:name="com.springspree.nitw.springspree2019.ProfileActivity" />
    </application>

</manifest>